<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Hello, World! - A-Frame</title>
    <meta name="description" content="Hello, World! - A-Frame">
    <script src="javascripts/aframe.js"></script>
  </head>
  <body>
    <a-scene>
      <a-assets>
        <img  id = "groundTexture"  src = "https://cdn.aframe.io/a-painter/images/floor.jpg" >
      </a-assets>
      <a-box  position="-1 0.5 -3" rotation="0 45 0" color="#4CC3D9">
        <a-animation  attribut ="position"  from="-1 0.5 -3"  to="9 9 89" > </a-animation >
        <a-animation  attribute = "material.color"  from = "#4CC3D9" to = "red"  dur = "1000" > </a-animation >
      </a-box>

      <a-assets >
        <a-asset-item  id ="tree"  src ="Cesium/gltf/02.gltf" > </a-asset-item >
      </a-assets >
      <a-entity  gltf-model = "#tree"  position="-1 0.5 3" rotation="0 45 0" color="#4CC3D9"> </a-entity >

      <a-sphere position="3 3 -5" radius="1.25" src = "https://i.imgur.com/mYmmbrp.jpg" ></a-sphere>
      <a-sphere src = "/images/s2.png" position="0 1.25 -5" radius="1.25"></a-sphere>
      <a-cylinder  position="1 0.75 -3" radius="0.5" height="1.5" color="#FFC65D"></a-cylinder>
      <a-plane src = "/images/s1.png" rotation = "-90 0 0"  width = "40"  height ="15"></a-plane>
      <a-sky color="#ECECEC"></a-sky>
    </a-scene>
  </body>
</html>